<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Содержание</h1>
  </div>

  <div data-role="content">
    <table>
      <tr>
        <% @categories.each do |obj| %>
          <td class="recordLabel"><%= link_to "#{obj.name}", :action => :show, :id => obj.object %></td>
        <% end %>
      </tr>
    </table>
  </div>

</div>
